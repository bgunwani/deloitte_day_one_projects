
@{
    ViewData["Title"] = "Info";
}

@{
    TempData.Peek("Message");
}

@*<h3>@TempData["Message"]</h3>*@
<p>There are @TempData["StudentCount"] students added into the list.</p>

<table class="table">
    <thead>
        <tr>
            <th>Student Id</th>
            <th>Student Name</th>
            <th>Student Age</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in TempData["Students"] as List<Student>)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Name</td>
                <td>@item.Age</td>
            </tr>
        }
    </tbody>
</table>

<hr/>

@*@{
    TempData.Keep("Message");
}
*@
<a asp-controller="Student" asp-action="TestOne">Click Me</a>

